{
  "name": "Dusk",
  "version": "1.0.0",
  "description": "Discord clone",
  "engines": {
    "node": "10.12.x"
  },
  "scripts": {
    "start": "node server",
    "dev": "concurrently --kill-others \"cd react-ui && npm run start\" \"nodemon server\" ",
    "heroku-postbuild": "cd react-ui/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "axios": "^0.18.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "email-validator": "^2.0.4",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.4",
    "mongoose-validator": "^2.1.0",
    "socket.io": "^2.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Corned/dusk.git"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4"
  }
}
